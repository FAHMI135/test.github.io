</div>
                <div id="privacy-pop-up" class="eucookie-pop-up" style="display: none;">
                    <div class="content container-fluid">
                        <div class="row">
                            <div class="col-lg-11 mb-3">
                                We use cookies to improve your experience. By clicking 'Accept', you agree to the                                 <a href="/en/company/terms">
                                    Cookies Policy                                </a>
                            </div>
                            <div class="col-lg-1">
                                <button class="btn btn-default bg-brand btn-small text-white btn-readmore" id="exit-popup">
                                    <span>
                                        Accept                                    </span>
                                </button>
                            </div>
                        </div>
                    </div>
                </div>

                <script type="text/javascript">
                    jQuery(document).ready(function() {

                        // Create cookie
                        function setCookie(cname, cvalue, exdays) {
                            const d = new Date();
                            d.setTime(d.getTime() + (exdays*24*60*60*1000));
                            let expires = "expires="+ d.toUTCString();
                            document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
                        }

                        // Delete cookie
                        function deleteCookie(cname) {
                            const d = new Date();
                            d.setTime(d.getTime() + (24*60*60*1000));
                            let expires = "expires="+ d.toUTCString();
                            document.cookie = cname + "=;" + expires + ";path=/";
                        }

                        // Read cookie
                        function getCookie(cname) {
                            let name = cname + "=";
                            let decodedCookie = decodeURIComponent(document.cookie);
                            let ca = decodedCookie.split(';');
                            for(let i = 0; i <ca.length; i++) {
                                let c = ca[i];
                                while (c.charAt(0) == ' ') {
                                    c = c.substring(1);
                                }
                                if (c.indexOf(name) == 0) {
                                    return c.substring(name.length, c.length);
                                }
                            }
                            return "";
                        }
        
                        if(localStorage.getItem('eucookie') == 'cookie_Mozilla Firefox v.?_Unknown'){
                            jQuery("#privacy-pop-up").delay(1).fadeIn(1000);
                            localStorage.setItem('eucookie','Mozilla Firefox v.?_Unknown');
                        }
                        
                        jQuery('#exit-popup').click(function(e) { 
                            deleteCookie('cookie_Mozilla Firefox v.?_Unknown');
                            setCookie('cookie_Mozilla Firefox v.?_Unknown', 1, 30);
                            jQuery('#privacy-pop-up').fadeOut(1000);
                        });

                        let cookie_consent = getCookie("cookie_Mozilla Firefox v.?_Unknown");
                        if(cookie_consent == ""){
                            jQuery('#privacy-pop-up').removeClass('d-none');
                            jQuery('#privacy-pop-up').delay(1).fadeIn(1000);
                        }else{
                            jQuery('#privacy-pop-up').addClass('d-none');
                        }
                    
                    });
                </script>
            </main>
            
            
            
