                $( document ).ready(function() {
                    $(".control").click(function(){
                        $(".form").show();
                        $(".sb").focus();
                        $(".form").removeClass("come-out");
                    });

                    $(".sclose").click(function() {
                        $(".form").hide();
                        $(".form").addClass("come-out");
                        $(".sb").val("");
                        $("#live-search-list").hide();
                    });
                });

                $(document).ready(function(){
                    $("#sb").on("keyup", function() {
                        if($(this).val().length > 0){

                            $("#live-search-list").show();
                            $("#feedback-search").fadeOut();
                            $("#feedback-search").html("");

                            var value = $(this).val().toLowerCase();
                            $("#live-search-list li").filter(function() {
                                $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
                            });

                        } else {
                            $("#live-search-list").hide();
                            $("#feedback-search").fadeIn();
                            $("#feedback-search").html("Suggestion : TOWING");
                        }
                    });
                });
            
